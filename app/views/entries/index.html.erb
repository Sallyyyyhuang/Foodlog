<h1>Entries</h1>
<table>
  <thead>
    <tr>
      <th>Meal type</th>
      <th>Calories</th>
      <th>Proteins</th>
      <th>Fats</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= enrty.meal_type %></td>
        <td><%= enrty.calories %></td>
        <td><%= enrty.proteins %></td>
        <td><%= enrty.carbohydrates %></td>
        <td><%= enrty.fats %></td>
        <td><%= link_to 'Show', entry %></td>
        <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
        <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' }
        %></td>
      </tr>
    <% end %>
  </tbody>
  <%= link_to "New entry", new_entry_path %>
</table>



